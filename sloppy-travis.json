{
  "project": "clean-car",
  "services": [
    {
      "id": "apps",
      "apps": [
        {
          "id": "cleancar",
          "image": "$docker_image",
          "instances": 1,
          "mem": 512,
          "domain": {
            "uri": "$domain",
            "redirectHttps": true
          },
          "ssl": true,
          "port_mappings": [
            {
              "container_port": 3333
            }
          ],
          "volumes": [
          ],
          "health_checks": [
          ],
          "logging": null
        }
      ]
    }
  ]
}
